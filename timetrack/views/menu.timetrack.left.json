{
    "name": "Timetrack left menu",
    "layout": {
        "items": [
            {
                "id": "time_entries",
                "type": "entry",
                "label": "Time entries",
                "icon": "timer",
                "description": "Time entries.",
                "children": [
                    {
                        "id": "time_entries.month_overview",
                        "type": "entry",
                        "label": "Month overview",
                        "icon": "task_alt",
                        "description": "All entries for current month.",
                        "context": {
                            "entity": "timetrack\\TimeEntry",
                            "view": "list.default",
                            "order": "created",
                            "sort": "desc",
                            "domain": [
                                ["date", ">=", "date.this.month.first()"],
                                ["date", "<=", "date.this.month.last()"]
                            ]
                        }
                    },
                    {
                        "id": "time_entries.ready",
                        "type": "entry",
                        "label": "To validate",
                        "icon": "task_alt",
                        "description": "Entries waiting to be validated.",
                        "context": {
                            "entity": "timetrack\\TimeEntry",
                            "view": "list.default",
                            "order": "created",
                            "sort": "asc",
                            "domain": [
                                [
                                    "status",
                                    "=",
                                    "ready"
                                ]
                            ]
                        }
                    },
                    {
                        "id": "time_entries.billable",
                        "type": "entry",
                        "label": "To bill",
                        "icon": "payments",
                        "description": "Entries waiting to be invoiced.",
                        "context": {
                            "entity": "timetrack\\TimeEntry",
                            "view": "list.billable",
                            "order": "created",
                            "sort": "asc"
                        }
                    },
                    {
                        "id": "time_entries.advanced",
                        "type": "entry",
                        "label": "Advanced",
                        "icon": "tune",
                        "description": "",
                        "children": [
                            {
                                "id": "time_entries.all",
                                "type": "entry",
                                "label": "All",
                                "icon": "list",
                                "description": "All time entries",
                                "context": {
                                    "entity": "timetrack\\TimeEntry",
                                    "view": "list.default",
                                    "order": "created",
                                    "sort": "desc"
                                }
                            },
                            {
                                "id": "time_entries.billed",
                                "type": "entry",
                                "label": "Billed",
                                "icon": "price_check",
                                "description": "Entries waiting to be validated.",
                                "context": {
                                    "entity": "timetrack\\TimeEntry",
                                    "view": "list.default",
                                    "order": "created",
                                    "sort": "asc",
                                    "domain": [
                                        [
                                            "status",
                                            "=",
                                            "billed"
                                        ]
                                    ]
                                }
                            },
                            {
                                "id": "time_entries.archived",
                                "type": "entry",
                                "label": "Archived",
                                "icon": "inventory",
                                "description": "Archived entries",
                                "context": {
                                    "entity": "timetrack\\TimeEntry",
                                    "view": "list.default",
                                    "order": "created",
                                    "sort": "desc",
                                    "domain": [
                                        [
                                            "state",
                                            "=",
                                            "archive"
                                        ]
                                    ]
                                }
                            }
                        ]
                    }
                ]
            },
            {
                "id": "projects",
                "type": "parent",
                "label": "Projects",
                "icon": "dataset",
                "description": "Projects",
                "children": [
                    {
                        "id": "projects.all",
                        "type": "entry",
                        "label": "All",
                        "icon": "list",
                        "description": "All projects",
                        "context": {
                            "entity": "timetrack\\Project",
                            "view": "list.default",
                            "sort": "desc",
                            "order": "id"
                        }
                    },
                    {
                        "id": "projects.archived",
                        "type": "entry",
                        "label": "Archived",
                        "icon": "inventory",
                        "description": "Archived projects",
                        "context": {
                            "entity": "timetrack\\Project",
                            "view": "list.archived",
                            "sort": "desc",
                            "order": "id"
                        }
                    }
                ]
            },
            {
                "id": "settings",
                "type": "parent",
                "label": "Settings",
                "icon": "settings",
                "description": "Timetrack settings.",
                "children": [
                    {
                        "id": "settings.time_entry_sale_model",
                        "type": "entry",
                        "label": "Sale Models",
                        "icon": "calculate",
                        "context": {
                            "entity": "timetrack\\TimeEntrySaleModel",
                            "view": "list.default"
                        }
                    }
                ]
            }
        ]
    }
}